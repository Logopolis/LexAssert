<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<VersionPrefix>0.1.0</VersionPrefix>
	<Authors>Chris Molloy Wischer</Authors>
	<Copyright>Share and Enjoy</Copyright>
	<PackageProjectUrl>logopolis.uk/packages/lexassert</PackageProjectUrl>
	<RepositoryUrl>https://github.com/Logopolis/LexAssert</RepositoryUrl>
	<PackageTags>xUnit;Assert</PackageTags>
	<Description>Some useful equality Asserts for use with xUnit.</Description>
	<IsPackable>true</IsPackable>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="xunit" Version="2.4.1" />
  </ItemGroup>

</Project>
